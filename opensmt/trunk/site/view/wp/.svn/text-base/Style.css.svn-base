}
.adminmenu {
    float: left;
    clear: left;
    width: 145px;
    margin-top: 15px;
    margin-right: 5px;
    margin-bottom: 15px;
    margin-left: -160px;
    position: relative;
    padding: 0;
    list-style: none;
}
.innercontent {
    clear: both;
    margin-left: 175px;
}
.admin-panel {
    float: left;
    width: 100%;
}
.adminmenu * {
    -moz-user-select: none;
}
.adminmenu li.menu-separator {
    cursor: w-resize;
    height: 21px;
    margin: 0;
    padding: 0;
}
.adminmenu li.menu-top {
