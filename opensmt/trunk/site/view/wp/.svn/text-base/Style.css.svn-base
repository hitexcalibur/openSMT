    -webkit-border-bottom-left-radius: 12px;
    -khtml-border-bottom-right-radius: 12px;
    -khtml-border-bottom-left-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
}
.favorite-actions a {
    display: block;
